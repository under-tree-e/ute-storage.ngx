{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "paths": {
      "ngx-ute-storage": [
        "dist/ngx-ute-storage"
      ],
      "@interfaces/*": [
        "projects/ngx-ute-storage/src/lib/interfaces/*"
      ],
      "@services/*": [
        "projects/ngx-ute-storage/src/lib/services/*"
      ],
      "@app/*": [
        "projects/ngx-ute-storage/src/lib/*"
      ]
    },
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ]
  },
  "typedocOptions": {
    "entryPoints": [
      "./projects/ngx-ute-storage/src/public-api.ts"
    ],
    "out": "dist/docs"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}